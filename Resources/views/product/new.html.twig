{% extends '@JasderoPassePlat/main/adminLayout.html.twig' %}

{% block passe_plat_content %}
<div class="container">
    <div class="row">
        <h5 class="center-align">{{ content }} creation</h5>
        {{ form_start(form) }}
        {{ form_widget(form) }}
    <button class="btn waves-effect waves-light" type="submit" name="action">Create
        <i class="material-icons right">send</i>
    </button>
    {{ form_end(form) }}
    <ul>
        <li>
            <a href="{{ path('product_index') }}" class="btn">Back to the list</a>
        </li>
    </ul>
    </div>
</div>
{% endblock %}
